(function () {
  var dialog = new Dialog({caption: '<%= t 'outlay_record.edit_form_title' %>'});
  $(dialog.content('<%= escape_javascript(render(:partial => 'form')) %>')).find('input[type=submit]').remove();
  dialog.show();
  dialog.buttons.commit.innerHTML = '<%= t 'common.save' %>';
  dialog.buttons.cancel.innerHTML = '<%= t 'common.cancel' %>';
})();
